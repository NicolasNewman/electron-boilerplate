    "@typescript-eslint/eslint-plugin": "^1.11.0",
    "@typescript-eslint/parser": "^1.11.0",
    "css-loader": "^3.0.0",
    "electron": "^5.0.6",
    "eslint": "^6.0.1",
    "eslint-plugin-react": "^7.14.2",
    "file-loader": "^4.1.0",
    "google-fonts-plugin": "^4.1.0",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.8.0",
    "rimraf": "^2.6.3",
    "style-loader": "^0.23.1",
    "ts-loader": "^6.0.4",
